filetype plugin indent on

colorscheme desert
syntax on

set nowrap

set hlsearch
set ignorecase
set smartcase

set autoindent

set ruler
set number
set list
set wildmenu

set shiftwidth=4
set softtabstop=4
set expandtab
set tabstop=2
set smarttab

set clipboard=unnamed

set nobackup

set background=black
