#
# 環境設定
set -x EDITOR nvim
set -x LANG ja_JP.UTF-8
set -x TERM xterm-256color 

set -x LESSCHARSET utf-8

set -x PATH /usr/local/opt/gnu-tar/libexec/gnubin $PATH

# rust 関連
set -x PATH /Users/dd/.cargo/bin $PATH
set -x RUST_SRC_PATH ~/.multirust/toolchains/stable-x86_64-apple-darwin/lib/rustlib/src/rust/src

alias vim="nvim"

alias rm="rm -i"
alias cp="cp"
alias mv="mv -i"
alias mkdir="mkdir -p"
